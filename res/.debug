<?xml version="1.0" encoding="UTF-8"?>
<ExtensionList>
  <% for (var i = 0; i < build.extensions.length; i++) { %>
  <Extension Id="<%= build.extensions[i].id %>">
    <HostList>
      <!-- Photoshop -->
      <Host Name="PHXS" Port="<%= build.launch.host_port + (i * 100) + 0 %>"/>

      <!-- Illustrator -->
      <Host Name="ILST" Port="<%= build.launch.host_port + (i * 100) + 1 %>"/>

      <!-- InDesign -->
      <Host Name="IDSN" Port="<%= build.launch.host_port + (i * 100) + 2 %>" />

      <!-- After Effects -->
      <Host Name="AEFT" Port="<%= build.launch.host_port + (i * 100) + 3 %>" />

      <!-- Premiere -->
      <Host Name="PPRO" Port="<%= build.launch.host_port + (i * 100) + 4 %>" />

      <!-- Prelude -->
      <Host Name="PRLD" Port="<%= build.launch.host_port + (i * 100) + 5 %>" />

      <!-- Flash -->
      <Host Name="FLPR" Port="<%= build.launch.host_port + (i * 100) + 6 %>" />

      <!-- Dreamweaver -->
      <Host Name="DRWV" Port="<%= build.launch.host_port + (i * 100) + 7 %>" />

      <!-- InCopy -->
      <Host Name="AICY" Port="<%= build.launch.host_port + (i * 100) + 8 %>" />
    </HostList>
  </Extension>
  <% } %>
</ExtensionList>
